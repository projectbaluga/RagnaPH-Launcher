[Main]
# Whether patching is allowed.
allow=true
# Allow starting the game even if patching is disabled.
Force_Start=false
# Message shown when patching is not allowed.
policy_msg=Server under maintenance.
# Base URL where patch files are hosted.
file_url=https://ragna.ph/patch/

[Patch]
# Name of the patch list file to download from file_url.
PatchList=patchlist.txt
